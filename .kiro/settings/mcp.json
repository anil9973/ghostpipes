{
	"mcpServers": {
		"aws-docs": {
			"command": "uvx",
			"args": ["awslabs.aws-documentation-mcp-server@latest"],
			"env": {
				"FASTMCP_LOG_LEVEL": "ERROR"
			},
			"disabled": false,
			"autoApprove": ["search_aws_documentation", "get_aws_documentation"]
		},
		"aws-kb-retrieval": {
			"command": "uvx",
			"args": ["mcp-server-aws-kb-retrieval"],
			"env": {
				"AWS_REGION": "us-east-1",
				"FASTMCP_LOG_LEVEL": "ERROR"
			},
			"disabled": false,
			"autoApprove": ["retrieve_and_generate"]
		},
		"filesystem": {
			"command": "uvx",
			"args": ["mcp-server-filesystem", "${workspaceFolder}"],
			"env": {
				"FASTMCP_LOG_LEVEL": "ERROR"
			},
			"disabled": false,
			"autoApprove": ["read_file", "list_directory", "search_files"]
		},
		"postgres": {
			"command": "uvx",
			"args": ["mcp-server-postgres"],
			"env": {
				"POSTGRES_CONNECTION_STRING": "${env:DATABASE_URL}",
				"FASTMCP_LOG_LEVEL": "ERROR"
			},
			"disabled": false,
			"autoApprove": ["query", "list_tables", "describe_table"]
		},
		"git": {
			"command": "uvx",
			"args": ["mcp-server-git"],
			"env": {
				"FASTMCP_LOG_LEVEL": "ERROR"
			},
			"disabled": false,
			"autoApprove": ["git_status", "git_diff", "git_log"]
		}
	}
}
